{
  "name": "auto_caravan_sport",
  "version": "1.0.0",
  "description": "A small business website",
  "author": "Radek Palisa",
  "scripts": {
    "dev": "parcel src/jade/index.pug",
    "build": "parcel build src/jade/index.pug"
  },
  "devDependencies": {
    "autoprefixer": "9.8.6",
    "parcel-bundler": "^1.12.4",
    "pug": "^3.0.1",
    "sass": "^1.26.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "> 1%",
          "last 4 versions",
          "ie >= 10"
        ]
      },
      "cssnano": {
        "preset": [
          "default",
          {
            "discardComments": {
              "removeAll": true
            }
          }
        ]
      }
    }
  }
}
